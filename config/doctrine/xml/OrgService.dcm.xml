<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OrgService" table="org_service">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="passportId" type="integer" column="passport_id" nullable="false"/>
    <field name="serviceTypeId" type="integer" column="service_type_id" nullable="false"/>
    <field name="name" type="string" column="name" length="45" nullable="false"/>
    <field name="code" type="string" column="code" length="45" nullable="false"/>
    <field name="targetType" type="boolean" column="target_type" nullable="false"/>
    <field name="intro" type="text" column="intro" nullable="false"/>
  </entity>
</doctrine-mapping>
